{
  "name": "circulaw",
  "version": "1.0.0",
  "description": "regelgeving voor een circulaire economie",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "concurrently \"next dev\" \"cd studio && yarn dev\"",
    "build": "echo 'Building Sanity to public/studio' && cd studio && yarn && npx @sanity/cli build ../public/studio -y && cd .. && next build",
    "start": "vercel dev",
    "clean": "prettier --write \"(pages|components)/**/*.+(js|jsx|json|css|md)\" --config ./.prettier.json",
    "lint": "next lint",
    "prepare": "husky install",
    "pre-commit": "next lint --fix"
  },
  "dependencies": {
    "@emailjs/browser": "^3.7.0",
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5.2",
    "fuse.js": "^6.6.2",
    "next": "^12.0.7",
    "next-pagination": "^3.1.0",
    "next-sanity": "^3.1.3",
    "next-sanity-image": "^5.0.0",
    "open-graph-scraper": "^4.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scroll": "^1.8.7",
    "swr": "^1.0.1",
    "use-persisted-state": "^0.3.3"
  },
  "devDependencies": {
    "@sanity/cli": "^3.0.6",
    "autoprefixer": "^10.2.6",
    "concurrently": "^7.6.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lerna": "^6.1.0",
    "postcss": "^8.3.5",
    "prettier": "2.5.1",
    "tailwindcss": "^2.2.4"
  }
}
