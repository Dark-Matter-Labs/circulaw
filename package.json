{
  "name": "circulaw",
  "version": "2.0.0",
  "description": "regelgeving voor een circulaire economie",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "next dev",
    "dev:staging": "env-cmd -e staging next dev",
    "dev:prod": "env-cmd -e production next dev",
    "cms:staging": "env-cmd -e staging concurrently \"next dev\" \"cd studio && yarn && yarn dev:staging\"",
    "cms:prod": "env-cmd -e production concurrently \"next dev\" \"cd studio && yarn && yarn dev:prod\"",
    "prebuild": "echo 'Building Sanity to public/studio' && cd studio && yarn && npx @sanity/cli build ../public/studio -y && echo 'Done'",
    "build": "next build",
    "build:staging": "env-cmd -e staging next build",
    "build:prod": "env-cmd -e production next build",
    "start": "next start",
    "clean": "prettier --write \"(pages|components|studio|lib)/**/*.+(js|jsx|json|css|md)\" --config ./.prettier.json",
    "lint": "next lint",
    "prepare": "husky install",
    "pre-commit": "next lint --fix"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^1.0.6",
    "@portabletext/react": "^2.0.0",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/image-url": "^1.0.1",
    "@tailwindcss/forms": "^0.5.2",
    "@vercel/og": "^0.5.6",
    "axios": "^1.4.0",
    "cookies-next": "^2.1.1",
    "fuse.js": "^6.6.2",
    "next": "^13.1.1",
    "next-pagination": "^3.1.0",
    "next-sanity": "^3.1.3",
    "next-sanity-image": "^5.0.0",
    "next-share": "^0.20.0",
    "nprogress": "^0.2.0",
    "open-graph-scraper": "^4.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotjar": "^5.1.0",
    "react-icons": "^4.7.1",
    "react-lottie": "^1.2.3",
    "react-scroll": "^1.8.7",
    "react-social-icons": "^5.15.0",
    "react-xarrows": "^2.0.2",
    "swr": "^1.0.1",
    "use-persisted-state": "^0.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.4.1",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "postcss": "^8.4.21",
    "prettier": "2.5.1",
    "tailwindcss": "^3.2.7"
  }
}
